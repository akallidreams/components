{
  "name": "@akalli/components",
  "version": "0.0.7",
  "main": "index.ts",
  "files": [
    "src/*.tsx",
    "src/**/&.tsx",
    "src/**/*",
    "index.ts"
  ],
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "storybook-start": "start-storybook -p 8080 ./src -c .storybook",
    "storybook-build": "build-storybook -c .storybook -o ./dist/icons/storybook/",
    "server": "storybook-server",
    "build": "rimraf ./build && tsc --p tsconfig-build.json",
    "prod": "npm run build && npm publish",
    "lint": "eslint -c .eslintrc.js --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "clear": "rm -rf node_modules && npm install",
    "test:watch": "jest --watch --coverage=false --changedSince=origin/master",
    "testDebug": "jest -o --watch --coverage=false",
    "test": "jest --passWithNoTests",
    "snapshots": "jest -u --coverage=false"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/webpack-config": "^0.16.27",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.26",
    "@storybook/cli": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.7.1",
    "typescript": "~4.3.5"
  }
}
